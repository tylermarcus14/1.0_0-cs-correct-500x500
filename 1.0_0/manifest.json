{
   "browser_action": {
      "default_icon": {
         "19": "icon19.png",
         "38": "icon38.png"
      },
      "default_popup": "popup.html",
      "default_title": "Shopify Bot"
   },
   "background": {
       "scripts": ["background.js"],
       "persistent": false
   },
   "content_scripts": [ {
      "js": [ "jquery-2.1.4.min.js", "content.js" ],
      "matches": [ "<all_urls>" ]
   } ],
   "description": "Allows users to see inventory info on Shopify products",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Shopify Item Extension",
   "permissions": [ "activeTab" ],
   "version": "1.0"
}
